{
  "name": "insights-app",
  "description": "Santiment Insights",
  "version": "0.0.1",
  "scripts": {
    "dev": "GIT_HEAD=$GIT_HEAD BACKEND_URL=https://api-stage.santiment.net sapper dev",
    "build": "GIT_HEAD=$GIT_HEAD NODE_ENV=production sapper build",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "lint:js": "yarn eslint src/**/*.js",
    "prettify:js": "yarn prettier --write src/**/*.js",
    "prettify:svelte": "yarn prettier --write src/**/*.svelte --parser=html",
    "postinstall": "patch-package",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,test,stories}/**/*.{js,ts}": [
      "prettier --write src/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify:svelte & npm run prettify:js & npm run lint:js",
      "pre-push": "npm run prettify:svelte & npm run prettify:js & npm run lint:js"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "dependencies": {
    "san-webkit": "https://github.com/santiment/san-webkit.git#45b0180",
    "@santiment-network/ui": "^0.5.177",
    "apollo-boost": "^0.4.0",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.14",
    "compression": "^1.7.1",
    "copy-to-clipboard": "^3.2.0",
    "draft-convert": "^2.1.8",
    "draft-js": "^0.10.5",
    "es6-promise": "^4.2.6",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.13",
    "lodash.debounce": "^4.0.8",
    "medium-draft": "^0.5.17",
    "mixin-deep": "^2.0.1",
    "mobile-detect": "^1.4.3",
    "nprogress": "^0.2.0",
    "patch-package": "^6.1.2",
    "polka": "^0.5.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.7.0",
    "raven-js": "^3.27.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-virtualized-select": "^3.1.3",
    "rollup-plugin-alias": "^1.5.1",
    "sanitize-html": "^1.20.1",
    "set-value": "^3.0.1",
    "sirv": "^1.0.10",
    "svelte-preprocess-css-global": "^0.0.1",
    "web3": "0.20"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@modular-css/svelte": "^24.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "glob": "^7.1.4",
    "husky": "^4.2.5",
    "ngrok": "^3.1.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.16",
    "postcss-modules": "^1.4.1",
    "prettier": "^2.0.5",
    "pug": "^2.0.3",
    "rollup": "^1.21.2",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "0.27.8",
    "svelte": "^3.24.0",
    "svelte-preprocess": "2.12.0",
    "svg-sprite": "^1.5.0"
  }
}
